package com.company;

public class Main{
    public static void main(String args[]){
        int a[]={1,2,13,15,16};
        System.out.println("Odd Numbers:");
        for(int i=0;i<a.length;i++){
            if(a[i]%2!=0){
                System.out.println(a[i]);
            }
        }
            }
        }
