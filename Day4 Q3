package com.company;

import java.util.Scanner;

public class Main {

    public static int i;

    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        int array[] = new int[5];
        int sum=0;
        System.out.println("enter 5 digit to get sum");
        for(int i=0; i<5; i++){
            array[i] = sc.nextInt();
            sum = sum + array[i];
        }
        System.out.println("Sum of the elements of the array ::"+sum);
    }


    }

